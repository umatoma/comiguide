<ul class="media-list">
	<% @users.each do |user| %>
  <li class="media">
    <a class="pull-left" href="#">
      <img class="media-object" src="http://placehold.jp/64x64.png">
    </a>
    <div class="media-body">
      <h4 class="media-heading"><%= user.username %></h4>
      <p><%= user.email %></p>
    </div>
  </li>
  <% end %>
</ul>