<% set_page_sub_title t('views.ccircle_checklists.index.title', comiket_id: @comiket.id) %>

<div id="ccircle_checklists_controller"
     ng-controller="CcircleChecklistsCtrl"
     data-comiket-id="<%= @comiket.id %>">

  <%= render 'share/page_title' %>

  <div class="row">
    <div class="col-md-2">

    </div>
    <div class="col-md-5">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">CreateCcirckeChecklist</h3>
        </div>
        <div class="panel-body">
          <%= render 'ccircle_checklists/form_create' %>
        </div>
      </div>
    </div>
    <!-- //.col -->
    <div class="col-md-5">
      <%= render 'checklists', comiket: @comiket %>
    </div>
    <!-- //.col -->
  </div>
</div>
